<!-- ServerController.fxml -->
<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ServerController">
    <center>
        <TextArea fx:id="serverTextArea" prefHeight="400.0" prefWidth="600.0" wrapText="true" />
    </center>
    <bottom>
        <HBox alignment="CENTER" spacing="20">
            <Button text="Login Successful" onAction="#loginSuccessful" />
            <Button text="Login Not Successful" onAction="#loginNotSuccessful" />
        </HBox>
    </bottom>
</BorderPane>
